#!/usr/bin/env sh
# https://bitbucket.org/jasonwryan/centurion/src/120ca5085493f49e6ed0c3d42ef7992d8010e086/Scripts/reagent?at=default
for agent in /tmp/ssh-*/agent.*; do
    export SSH_AUTH_SOCK=$agent

    if ssh-add -l 2>&1 >/dev/null; then
        echo "Found working SSH Agent..."
        ssh-add -l
    else
        echo "Cannot find ssh agent"
    fi
done
